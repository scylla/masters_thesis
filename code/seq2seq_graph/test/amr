# ::alignments 1-1.1.2.2.1 2-1.1.1 3-1.1 4-1 5-1.2.r 6-1.2.1.2.1 7-1.2 8-1.2.2.1 9-1.3.2 10-1.3.2 11-1.3.2 12-1.3.2 13-1.3.r 15-1.3.1 16-1.3.4 16-1.3.4.r 17-1.3 19-1.3.2 20-1.3.3.r 22-1.3.3
(r / report-01 
      :ARG0 (a / agency 
            :mod (n / news) 
            :mod (c / country :wiki "Iran" 
                  :name (n2 / name :op1 "Iran"))) 
      :ARG1 (s / state-01 
            :ARG0 (p / person :wiki "Mahmoud_Ahmadinejad" 
                  :name (n3 / name :op1 "Ahmadinejad")) 
            :ARG1 (h / have-03 
                  :ARG0 c 
                  :ARG1 (c2 / centrifuge :quant 6000 
                        :ARG1-of (w / work-09)))) 
      :concession-of (r2 / remove-01 
            :ARG0 a 
            :ARG1 s 
            :ARG2 (i / internet) 
            :time (s2 / subsequent)))

